<Window x:Class="GraphEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraphEditor"
        mc:Ignorable="d"
        Title="GraphEditor" Height="450" Width="800"
        >
    <TabControl>
        <TabItem Header="Tab1" x:Name="Tab">
            <local:TabTemplate></local:TabTemplate>
        </TabItem>
        <TabItem Header="Tab2" x:Name="Tab_">
            <local:TabTemplate></local:TabTemplate>
        </TabItem>
    </TabControl>
</Window>
